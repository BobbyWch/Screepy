{
  "compilerOptions": {
    "incremental": true,
    "tsBuildInfoFile": ".tsbuildinfo",
    "target": "es2017",
    "moduleResolution": "Node",
    "outDir": "dist/",
    "baseUrl": "./",
    "sourceMap": false,
    "allowSyntheticDefaultImports": true,
//    "noUnusedLocals": true,
//    "strictNullChecks": true,
//    "alwaysStrict": true,
    "paths": {
      "@/*": ["./src/*"]
    },
    "removeComments": true
  },
  "exclude": [

  ],
  "include": [
    "src/**/*"
  ]
}